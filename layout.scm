(
 ("TOGGLE HELP" "..xx")
 ;; tap 1 finger
 (SPACE "..x.")
 (REPEAT "...x")
 (0 "x...") ;; RESERVED shift?
 (E ".x..")
 ;; tap 2 fingers
 (0 "x..x") ;; RESERVED ctrl?
 (A "x.x.")
 (T ".x.x")
 (O ".xx.")
 ;; delayed press right hand
 (I "x..."
    "x.x.")
 (D "x..."
    "x..x")
 (N ".x.."
    ".xx.")
 (H ".x.."
    ".x.x")
 ;; delayed press left hand
 (S "..x."
    "x.x.")
 (L "..x."
    ".xx.")
 (F "...x"
    "x..x")
 (G "...x"
    ".x.x")
 ;; delayed release right hand
 (R "x.x."
    "..x.")
 (M "x..x"
    "...x")
 (K ".xx."
    "..x.")
 (B ".x.x"
    "...x")
 ;; delayed release left hand
 (C "x.x."
    "x...")
 (J "x..x"
    "x...")
 (0 ".xx." ;; RESERVED
    ".x..")
 (P ".x.x"
    ".x..")
 ;; delayed press and release right hand
 (U "x..."
    "x.x."
    "..x.")
 (0 "x..." ;; RESERVED
    "x..x"
    "...x")
 (Y ".x.."
    ".xx."
    "..x.")
 (V ".x.."
    ".x.x"
    "...x")
 ;; delayed press and release left hand
 (W "..x."
    "x.x."
    "x...")
 (Q "..x."
    ".xx."
    ".x..")
 (X "...x"
    "x..x"
    "x...")
 (Z "...x"
    ".x.x"
    ".x..")
 ;; hold left and double-tap right hand
 (DEL   "...x"
        "x..x"
        "...x")
 (DEL   "xx..") ;; for those who can thumb trick
 (ENTER "...x"
        ".x.x"
        "...x")

 (DPAD_UP    ".x.."
             ".x.x"
             ".x..")
 (DPAD_DOWN  "x..."
             "x.x."
             "x...")
 (DPAD_LEFT  ".x.."
             ".xx."
             ".x..")
 (DPAD_RIGHT "x..."
             "x..x"
             "x..."))
